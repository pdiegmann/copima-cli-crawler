{
  "name": "copima-cli-crawler",
  "author": "",
  "description": "Stricli command line application",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "bin": {
    "copima-cli-crawler": "dist/cli.js",
    "__copima-cli-crawler_bash_complete": "dist/bash-complete.js"
  },
  "engines": {
    "bun": ">=1.2.8"
  },
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun run src/bin/cli.ts",
    "dev:auth": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun run src/bin/cli.ts auth --config examples/unified-config.yaml",
    "prebuild": "tsc -p src/tsconfig.json",
    "build": "tsup",
    "build:executables": "bun run scripts/build.ts",
    "build:clean": "rm -rf dist && bun run build:executables",
    "build:test": "bun run scripts/test-executables.ts",
    "prepublishOnly": "bun run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:bun": "bun test",
    "test:e2e": "bun run src/bin/cli.ts test",
    "test:e2e:basic": "bun run test:e2e examples/unified-config.yaml",
    "test:e2e:suite": "bun run test:e2e examples/test-configs/test-suite.yaml",
    "test:e2e:template": "bun run test:e2e examples/test-configs/template-test.yaml",
    "test:e2e:dry-run": "bun run test:e2e examples/test-configs/dry-run-test.yaml --dry-run",
    "release": "bun run scripts/release.ts",
    "release:local": "bun run scripts/release.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "codegen": "graphql-codegen-esm --config codegen.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "tsup": {
    "entry": [
      "src/bin/cli.ts",
      "src/bin/bash-complete.ts"
    ],
    "format": [
      "esm"
    ],
    "tsconfig": "src/tsconfig.json",
    "clean": true,
    "splitting": true,
    "minify": true
  },
  "lint-staged": {
    ".{ts,js}": "eslint — cache — fix",
    ".{ts,js,css,md}": "prettier — write",
    ".js": "eslint — cache — fix",
    ".{js,css,md}": "prettier — write"
  },
  "dependencies": {
    "@stricli/auto-complete": "^1.2.0",
    "@stricli/core": "^1.2.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.5",
    "eslint-plugin-sonarjs": "^3.0.5",
    "get-port": "^7.1.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jiti": "^2.6.0",
    "js-yaml": "^4.1.0",
    "lockfile": "^1.0.4",
    "open": "^10.2.0",
    "picocolors": "^1.1.1",
    "treeify": "^1.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@graphql-codegen/cli": "6.0.0",
    "@jest/globals": "^30.1.2",
    "@parcel/watcher": "^2.5.1",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/bun": "^1.2.22",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/node-fetch": "^2.6.13",
    "@types/treeify": "^1.0.3",
    "@typescript-eslint/parser": "^8.44.1",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/client-preset": "5.0.1"
  }
}
